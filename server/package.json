{
  "name": "node-inspector-websocket-proxy-server",
  "version": "0.2.0",
  "description": "Server component for Node.js Inspector WebSocket proxy",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js"
  },
  "keywords": [
    "inspector",
    "debugger",
    "websocket",
    "proxy",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "express": "^4.22.1",
